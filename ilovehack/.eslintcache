\"\r\n                            value={event.name}\r\n                            onChange={this.handleChange}\r\n                  />\r\n                  <label>Location:</label>\r\n                    <input\r\n                            type=\"text\"\r\n                            name=\"location\"\r\n                            value={event.location}\r\n                            onChange={this.handleChange}\r\n                  />\r\n                  <label>Date:</label>\r\n                    <input\r\n                            type=\"text\"\r\n                            name=\"date\"\r\n                            value={event.date}\r\n                            onChange={this.handleChange}\r\n                  />\r\n\r\n                    <input\r\n                    className=\"form_button_btn_edit\"\r\n                    type=\"submit\"\r\n                    value=\"Edit\"\r\n                  />\r\n                </div>\r\n\r\n                        </form>\r\n                    </div> \r\n                : \r\n                <>\r\n                {event.date ? \r\n                <div>\r\n                    <h1>{event.name}</h1>\r\n                     <p>{event.date.slice(0,16)} {event.location}</p>\r\n                </div>\r\n                   \r\n                : null}  \r\n                </>\r\n                 }\r\n                 <button onClick={() => this.joinThisEvent(user._id, event._id)}>Join</button>\r\n    </div>)\r\n}\r\n}\r\nexport default EventDetail;","C:\\Users\\eric_\\Desktop\\IronHack\\post-course\\ilovehack\\version2\\ILOVEHACK2.0-client\\ilovehack\\src\\components\\Events\\Events.jsx",[],"C:\\Users\\eric_\\Desktop\\IronHack\\post-course\\ilovehack\\version2\\ILOVEHACK2.0-client\\ilovehack\\src\\components\\SignUp\\Signup.jsx",["88","89","90","91","92","93","94"],"import React, { Component } from \"react\";\r\nimport { Link } from \"react-router-dom\";\r\nimport { withAuth } from \"../../lib/AuthProvider\";\r\