r(){\r\n        const {fullname, description, email, imgPath} = this.state\r\n       \r\n        return(\r\n            \r\n            <div>\r\n              <form onSubmit={this.handleFormSubmit} className='form-profile'>\r\n                <div>\r\n                  <img src={imgPath} alt=\"\" style={{ width: 100 }} />\r\n                </div>\r\n\r\n                <input type=\"file\" onChange={(e) => this.handleFileUpload(e)} className=\"upload-input\"/>\r\n                <div className=\"form_part\">\r\n                  <label>Full name:</label>\r\n                  <input\r\n                    type=\"text\"\r\n                    name=\"fullname\"\r\n                    value={fullname}\r\n                    onChange={(e) => this.handleChange(e)}\r\n                  />\r\n                </div>\r\n                <div className=\"form_part\">\r\n                  <label>Email:</label>\r\n                  <input\r\n                    type=\"text\"\r\n                    name=\"email\"\r\n                    value={email}\r\n                    onChange={(e) => this.handleChange(e)}\r\n                  />\r\n                </div>\r\n                <div className=\"form_part\">\r\n                  <label>Description:</label>\r\n                  <textarea\r\n                    type=\"text\"\r\n                    name=\"description\"\r\n                    value={description}\r\n                    onChange={(e) => this.handleChange(e)}\r\n                  />\r\n                </div>\r\n                <div className=\"form_button_container\">\r\n                  <input\r\n                    className=\"form_button_btn\"\r\n                    type=\"submit\"\r\n                    value=\"Edit\"\r\n                  />\r\n                </div>\r\n              </form>\r\n            </div>\r\n            \r\n        )\r\n    }\r\n}\r\nexport default Home;","C:\\Users\\marta\\Desktop\\Ilovehack2\\test\\ilovehack-client\\ilovehack\\src\\compone