 }\r\n      };\r\n    \r\n      handleFormSubmit = async (event) => {\r\n        try {\r\n          event.preventDefault();\r\n          const {fullname, description, email, imgPath} = this.state\r\n          await userservice.editUser({fullname, description, email, imgPath})\r\n          this.props.history.push('/home')\r\n        } catch (error) {\r\n          console.log(error, \"the error originated here\");\r\n        }\r\n      };\r\n\r\n    render(){\r\n        const {fullname, description, email, imgPath} = this.state\r\n       \r\n        return(\r\n            \r\n            <div className=\"form_container\">\r\n              <form onSubmit={this.handleFormSubmit}>\r\n                <div>\r\n                  <img src={imgPath} alt=\"\" style={{ width: 100 }} />\r\n                </div>\r\n\r\n                <input type=\"file\" onChange={(e) => this.handleFileUpload(e)} />\r\n                <div className=\"form_part\">\r\n                  <label>Full name:</label>\r\n                  <input\r\n                    type=\"text\"\r\n                    name=\"fullname\"\r\n                    value={fullname}\r\n                    onChange={(e) => this.handleChange(e)}\r\n                  />\r\n                </div>\r\n                <div className=\"form_part\">\r\n                  <label>Email:</label>\r\n                  <input\r\n                    type=\"text\"\r\n                    name=\"email\"\r\n                    value={email}\r\n                    onChange={(e) => this.handleChange(e)}\r\n                  />\r\n                </div>\r\n                <div className=\"form_part\">\r\n                  <label>Description:</label>\r\n                  <textarea\r\n                    type=\"text\"\r\n                    name=\"description\"\r\n                    value={description}\r\n                    onChange={(e) => this.handleChange(e)}\r\n                  />\r\n                </div>\r\n                <div className=\"form_button_container\">\r\n                  <input\r\n                    className=\"form_button_btn\"\r\n                    type=\"submit\"\r\n                    value=\"Edit\"\r\n                  />\r\n                </div>\r\n              </form>\r\n            </div>\r\n            \r\n        )\r\n    }\r\n}\r\nexport default Home;","C:\\Users\\eric_\\Desktop\\IronHack\\post-course\\ilovehack\\version2\\ILOVEHACK2.0-client\\ilovehack\\src\\pages\\landing.jsx",[],"C:\\Users\\eric_\\Desktop\\IronHack\\post-course\\ilovehack\\version2\\ILOVEHACK2.0-client\\ilovehack\\src\\components\\createEvent\\Create.jsx",[],"C:\\Users\\eric_\\Desktop\\IronHack\\post-course\\ilovehack\\version2\\ILOVEHACK2.0-client\\ilovehack\\src\\components\\LogIn\\Log-in.jsx",[],"C:\\Users\\eric_\\Desktop\\IronHack\\post-course\\ilovehack\\version2\\ILOVEHACK2.0-client\\ilovehack\\src\\components\\FAQ\\FAQ.jsx",[],"C:\\Users\\eric_\\Desktop\\IronHack\\post-course\\ilovehack\\version2\\ILOVEHACK2.0-client\\ilovehack\\src\\components\\ErrorPage\\Error.jsx",[],"C:\\Users\\eric_\\Desktop\\IronHack\\post-course\\ilovehack\\version2\\ILOVEHACK2.0-client\\ilovehack\\src\\components\\Onboarding\\Onboarding.jsx",[],"C:\\Users\\eric_\\Desktop\\IronHack\\post-course\\ilovehack\\version2\\ILOVEHACK2.0-client\\ilovehack\\src\\components\\Onboarding2\\Onboarding2.jsx",[],"C:\\Users\\eric_\\Desktop\\IronHack\\post-course\\ilovehack\\version2\\ILOVEHACK2.0-client\\ilovehack\\src\\components\\Home\\Home.jsx",["122","123"],"import { Component } from \"react\";\r\nimport React from \"react\";\r\nimport userservice from \"../../lib/user-service\";\r\nimport eventservice from \"../../lib/event-service\";\r\nimport { Link } from \"react-router-dom\";\r\n\r\nclass Home extends Component {\r\n    state = {\r\n        user: {},\r\n        events: []\r\n    }\r\n\r\n    getUser = async () =>{\r\n        try {\r\n            const theUser = await userservice.getUser()\r\n            const theEvents = await eventservice.getAllEvents()\r\n            this.setState({\r\n                user: theUser,\r\n                events: theEvents\r\n            })\r\n            \r\n        } catch (error) {\r\n            console.log(error)\r\n        }\r\n    }\r\n\r\n    componentDidMount(){\r\n        this.getUser()\r\n    }\r\n\r\n    render(){\r\n        const {user, events} = this.state\r\n       \r\n        return(\r\n            <div>\r\n                {user && user.fullname ? \r\n                <div className=\"main\">\r\n                    <h1>Hello {user.fullname}</h1>\r\n                    <div className=\"matches\">\r\n                        <h3>Matches</h3>\r\n                        {user.matches && user.matches.length !== 0 ? \r\n                        user.matches.map((person, index)=>{\r\n                            return <h6 key={index}>{person.fullname}</h6>\r\n                        }): <p>You don't have any matches yet. Check again tomorrow!</p>}\r\n                    </div>\r\n                    <div className=\"events\">\r\n                        <h3>My events:</h3>\r\n                        {events.map((event, index)=>{\r\n                            if(event.creator && event.creator === user._id){\r\n                                return <h5 key={index}><Link to={`/event/${event._id}`}>{event.name}</Link></h5>\r\n                            }\r\n                        })}\r\n                        <h3>Events I'm attending</h3>\r\n                        {events.map((event, index)=>{\r\n                            return <div key={index}>\r\n                            {event.attending ? event.attending.map((attendee, index)=>{\r\n                                if(attendee._id === user._id){\r\n                                    return <h5 key={index}><Link to={`/event/${event._id}`}>{event.name}</Link></h5>\r\n                                }\r\n                            }) :null}\r\n                            </div>\r\n                